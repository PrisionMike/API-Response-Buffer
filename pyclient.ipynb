    "import socket\n",
    "\n",
    "HOST = 'localhost'\n",
    "PORT = 23541\n",
    "\n",
    "with socket.socket(socket.AF_INET, socket.SOCK_STREAM) as s:\n",
    "    s.connect((HOST, PORT))\n",
    "    print(s)\n",
    "    s.sendall(b'GIVE 5')\n",
    "    data = s.recv(1024)\n",
    "\n",
    "print(\"Received: \", repr(data))"
